<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#12705d" />
            <gradient android:angle="-90" android:startColor="#0caf93" android:endColor="#0e8472"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#177463" />
            <solid android:color="#7b9e99"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#058997" />
            <gradient android:angle="-90" android:startColor="#077f81" android:endColor="#3b9385" />
        </shape>
    </item>
</selector>